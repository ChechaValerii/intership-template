{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 2021,
        "sourceType": "module"
    },
    "rules": {
        "indent":["error", 4],
        "no-underscore-dangle":[
            "error",
            {
                "allow":["_id"]
                }
            ]
    }
}
